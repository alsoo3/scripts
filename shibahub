zxp2 = 0
for i = 1, 65 do
zxp2 = zxp2 + 5
crater = Instance.new("Part",Torso)
crater.BrickColor = BrickColor.new("Bright yellow")
crater.Size = Vector3.new(5,5,5)
crater.Transparency = 1
crater.Name = "crator"
