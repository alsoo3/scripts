until stuck == true
end))
first = true
for i = 0,20 do
Swait()
  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,1.2 + 0.5 * COS(SINE/35),0.3)*ANGLES(RAD(-64.9+ 7 * SIN(SINE/35)),RAD(0),RAD(-180)),Alpha)
