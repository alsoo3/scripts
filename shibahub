-- xd(ab.p,15,0,0,0,true)
swait(.1)
end
end		
end))
end
for i = 0,26 do
Swait()
  	local Alpha = .2
  	RootJoint.C0 = RootJoint.C0:lerp(CF(-0.1,-1.2,0.3)*ANGLES(RAD(-108.9),RAD(8),RAD(-131.9)),Alpha)
  	LeftShoulder.C0 = LeftShoulder.C0:lerp(CF(-0.9,0.1,0.2)*ANGLES(RAD(-84.3),RAD(-48.5),RAD(-59.8)),Alpha)
