repeat
Swait()
pcall(function()
sp1.Parent = Effects
sp2.Parent = Effects
end)
sp1.CFrame = LeftArm.CFrame * CF(0,-1,0)
sp2.CFrame = RightArm.CFrame * CF(0,-1,0)
