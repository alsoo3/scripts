                local weld = it("Weld")
                weld.Parent = parent
                weld.Part0 = part0
                weld.Part1 = part1
                weld.C0 = c0
                return weld
            end
    
            function unanchor()
                if UNANCHOR == true then
                    g = CharacterA:GetChildren()
                    for i = 1, #g do
                        if g[i].ClassName == "Part" then
                            g[i].Anchored = false
                        end
                    end
                end
            end
    
