	if key == "c" then
		repeat
			swait()
			Chillmusic.Volume = Chillmusic.Volume - 0.05
		until Chillmusic.Volume == 0
		Chillmusic:Stop()
		swait()
		Chillmusic2:Play()
		Chillmusic.Volume = 1
	end
end)
