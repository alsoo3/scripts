-- Utility and stuff
tab3 = win:Tab("Misc", "http://www.roblox.com/asset/?id=0")

tab3:Label("LocalPlayer")
tab3:Line()

tab3:Slider("Walkspeed", "Makes you faster.", 0, 200,16,function(t)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (t)
end)

tab3:Slider("JumpPower", "Makes you jump higher", 0, 200,50,function(t)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = (t)
end)

tab3:Slider("Gravity", "Changes Your gravity", 0, 500,196,function(t)
game.Workspace.Gravity = (t)
end)

tab3:Button("GodMode", "Anyone cant kill you (BUT DON'T PRESS R)",  function()
loadstring(game:HttpGet('https://pastebin.com/raw/7i7CsTPe'))()
end)

tab3:Line()

tab3:Button("Rejoin", "rejoin game", function()
local ts = game:GetService("TeleportService")
local p = game:GetService("Players").LocalPlayer
ts:Teleport(game.PlaceId, p)
end)

tab3:Button("ServerHop", "serverhops to another server", function()
local HttpService, TPService = game:GetService("HttpService"), game:GetService("TeleportService")
local ServersToTP = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
for _, s in pairs(ServersToTP.data) do
   if s.playing ~= s.maxPlayers then
       TPService:TeleportToPlaceInstance(game.PlaceId, s.id)
   end
end
end)

tab3:Line()

tab3:Label("Script Hub")
tab3:Line()

