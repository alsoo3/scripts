local plr = game.Players.LocalPlayer
local char = plr.Character
char.Humanoid:Destroy()
Instance.new("Humanoid", char)

wait(game.Players.RespawnTime - .2)
wait(.2)
local pos = char.Torso.Position
wait(.3)
local char = plr.Character
char:MoveTo(pos)
char.Humanoid:Destroy()
Instance.new("Humanoid", char)
wait(game.Players.RespawnTime - .2)
wait(.2)
char:MoveTo(pos)
wait(.3)
loadstring(game:HttpGet("https://raw.githubusercontent.com/alsoo3/scripts/main/humandie"))();
